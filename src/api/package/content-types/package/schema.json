{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "shippment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shippment.shippment",
      "inversedBy": "packages"
    },
    "weight": {
      "type": "decimal",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true
    },
    "value": {
      "type": "decimal",
      "required": true
    },
    "trackingNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "packageStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Shipped",
        "Delivered"
      ],
      "default": "Pending"
    },
    "dimensions": {
      "type": "component",
      "repeatable": false,
      "component": "elements.dimension",
      "required": true
    },
    "packageCost": {
      "type": "decimal",
      "required": true
    }
  }
}
