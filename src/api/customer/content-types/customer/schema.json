{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "idNumber": {
      "type": "biginteger",
      "required": false
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "shippments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shippment.shippment",
      "mappedBy": "reciever"
    },
    "email": {
      "type": "email",
      "required": true
    }
  }
}
